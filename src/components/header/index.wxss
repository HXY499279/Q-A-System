.header {
  background-color: #30CB88;
  height: 75px;
  display: flex;
  justify-content: space-between;
  color: aliceblue;
}
.header .header-left {
  display: flex;
  align-items: center;
  line-height: 60px;
}
.header .header-left h1 {
  color: white;
  font-size: 28px;
  font-weight: bold;
  margin: 0 20px;
}
.header .header-left span {
  font-size: 18px;
  font-weight: bold;
}
.header .header-right {
  line-height: 60px;
  padding: 0px 20px;
}
.header .header-right a {
  color: white;
}
